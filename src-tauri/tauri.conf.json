{
  "productName": "Stork",
  "version": "0.1.0",
  "identifier": "com.stork.dev",
  "build": {
    "beforeDevCommand": "pnpm run dev",
    "beforeBuildCommand": "pnpm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "app": {
    "windows": [
      {
        "title": "Stork - Simple File Transfer",
        "width": 800,
        "height": 650,
        "resizable": true,
        "fullscreen": false,
        "minWidth": 680,
        "minHeight": 580
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all"
  }
}
