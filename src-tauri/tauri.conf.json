{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "Stork",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": true
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.example.stork"
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "Stork",
        "width": 800,
        "height": 600
      }
    ]
  }
}